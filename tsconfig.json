{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "removeComments": true,
    "strictNullChecks": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "./src/*"
      ],
    },
  }
}
